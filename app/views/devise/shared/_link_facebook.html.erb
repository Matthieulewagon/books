<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to omniauth_authorize_path(resource_name, provider), class: 'btn f-btn' do %>
        <i class="fa fa-facebook f-margin-right" ></i>
        Connecte-toi avec Facebook<br>
      <% end %>
  <% end -%>
<% end -%>
