<div class="col l8">
    <table>
      <thead>
        <tr>
          <th>List of Books</th>
          <th colspan="3"></th>
          <th>campus</th>
        </tr>
      </thead>
      <tbody>
        <% @books.each do |book| %>
          <tr>
            <td><%= book.price %></td>
            <td><%= book.title %></td>
            <td><%= book.description%> </td>
            <td><%= book.year %></td>
            <td><%= book.campus%></td>
          </tr>
        <% end %>
      </tbody>

    </table>

  </div>
